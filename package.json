{
  "name": "vui-button",
  "version": "1.2.1",
  "description": "Mixins and CSS for applying button styles",
  "scripts": {
    "autoprefix": "postcss -c postcss.config.json",
    "autoprefix:test": "postcss -c ./test/postcss.config.json",
    "clean": "rimraf *.css && rimraf ./test/*.css",
    "build:sass:test": "node-sass --importer ./node_modules/frau-sass-importer/ --output-style expanded ./test/test.scss ./test/test.css",
    "postbuild:sass:test": "npm run autoprefix:test",
    "build:sass": "node-sass --importer ./node_modules/frau-sass-importer/ --output-style expanded button.css.scss button.css",
    "postbuild:sass": "npm run autoprefix",
    "prebuild": "npm run clean",
    "build": "npm run build:sass && npm run build:sass:test && npm run screenshot",
    "perceptual-diff": "percy snapshot test/",
    "screenshot": "phantomjs ./test/screenshot.js",
    "pretest": "npm run build",
    "test": "csslint button.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-button.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "vui-colors": "^0.2.0",
    "vui-typography": "xbtsw/valence-ui-typography#experiment-npm-sass-importer"
  },
  "peerDependencies": {
    "frau-sass-importer": "^0.3.0"
  },
  "devDependencies": {
    "frau-sass-importer": "^0.3.0",
    "autoprefixer": "^6.1.2",
    "cpy": "^3.4.1",
    "csslint": "^0.10.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-cli": "^2.5.0",
    "rimraf": "^2.5.1"
  }
}
